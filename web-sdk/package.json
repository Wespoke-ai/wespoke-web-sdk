{
  "name": "@wespoke/web-sdk",
  "version": "0.1.4",
  "description": "Official JavaScript SDK for embedding Wespoke AI voice assistants in any website",
  "main": "dist/wespoke.cjs.js",
  "module": "dist/wespoke.esm.js",
  "unpkg": "dist/wespoke.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "examples",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "wespoke",
    "ai",
    "voice",
    "assistant",
    "livekit",
    "webrtc",
    "sdk",
    "embedding"
  ],
  "author": "Wespoke",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Wespoke-ai/wespoke-web-sdk.git",
    "directory": "web-sdk"
  },
  "bugs": {
    "url": "https://github.com/Wespoke-ai/wespoke-web-sdk/issues"
  },
  "homepage": "https://wespoke.ai",
  "dependencies": {
    "livekit-client": "^2.6.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "jest": "^29.7.0",
    "rollup": "^4.27.4",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "livekit-client": "^2.0.0"
  }
}
